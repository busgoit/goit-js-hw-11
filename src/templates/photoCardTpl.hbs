{{#each this}}
  <a href='{{largeImageURL}}' class='photo-card'>
    <img
      src='{{webformatURL}}'
      alt='{{tags}}'
      loading='lazy'
      width='{{webformatWidth}}'
      height='200'
    />
    <div class='info'>
      <p class='info-item'>
        <b>Likes: {{likes}}</b>
      </p>
      <p class='info-item'>
        <b>Views: {{views}}</b>
      </p>
      <p class='info-item'>
        <b>Comments: {{comments}}</b>
      </p>
      <p class='info-item'>
        <b>Downloads: {{downloads}}</b>
      </p>
    </div>
  </a>
{{/each}}